<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    {% block stylesheets %}
        {{ encore_entry_link_tags('addReply') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('addReply') }}
    {% endblock %}
    <style></style>
</head>
<body>
<div id="reply">
    <add-reply :post="{{ post|json_encode }}" :replies="{{ replies|json_encode }}" ></add-reply>
</div>
</body>

</html>